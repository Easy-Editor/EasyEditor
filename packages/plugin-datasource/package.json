{
  "name": "@easy-editor/plugin-datasource",
  "version": "1.0.1",
  "description": "DataSource plugin for EasyEditor",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "unpkg": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      }
    }
  },
  "homepage": "https://github.com/Easy-Editor/EasyEditor",
  "license": "MIT",
  "author": "JinSo <kimjinso@qq.com>",
  "keywords": [
    "@easy-editor",
    "easyeditor",
    "low-code",
    "editor",
    "engine",
    "plugin",
    "datasource"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Easy-Editor/EasyEditor"
  },
  "bugs": {
    "url": "https://github.com/Easy-Editor/EasyEditor/issues"
  },
  "scripts": {
    "format": "biome format --write .",
    "lint": "biome check .",
    "build": "npm-run-all -nl build:*",
    "build:clean": "rimraf dist/",
    "build:js": "rollup -c",
    "types": "npm-run-all -nl types:*",
    "types:src": "tsc --project tsconfig.build.json",
    "test-types": "tsc --project tsconfig.test.json"
  },
  "peerDependencies": {
    "@easy-editor/core": "workspace:^",
    "mobx": "^6.13.5"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "universal-request": "^2.2.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12"
  }
}
