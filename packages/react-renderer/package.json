{
  "name": "@easy-editor/react-renderer",
  "version": "1.0.1",
  "description": "React Renderer package for EasyEditor",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "unpkg": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      }
    }
  },
  "homepage": "https://github.com/Easy-Editor/EasyEditor",
  "license": "MIT",
  "author": "JinSo <kimjinso@qq.com>",
  "contributors": [
    {
      "name": "NoahCodeGG",
      "email": "noahcodegg@gmail.com",
      "url": "https://github.com/NoahCodeGG"
    }
  ],
  "keywords": [
    "@easy-editor",
    "easyeditor",
    "low-code",
    "editor",
    "engine",
    "react",
    "renderer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Easy-Editor/EasyEditor"
  },
  "bugs": {
    "url": "https://github.com/Easy-Editor/EasyEditor/issues"
  },
  "scripts": {
    "dev": "deno run --watch ./src/index.ts",
    "format": "biome format --write .",
    "lint": "biome check .",
    "build": "npm-run-all -nl build:*",
    "build:clean": "rimraf dist/",
    "build:js": "rollup -c",
    "types": "npm-run-all -nl types:*",
    "types:src": "tsc --project tsconfig.build.json",
    "test-types": "tsc --project tsconfig.test.json"
  },
  "peerDependencies": {
    "@easy-editor/core": "workspace:^",
    "@easy-editor/renderer-core": "workspace:^",
    "mobx-react": "^9.2.0",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "@types/react": "^18 || ^19",
    "@types/react-dom": "^18 || ^19"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12"
  }
}
