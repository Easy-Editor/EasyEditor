{
  "name": "@easy-editor/react-renderer",
  "version": "0.0.6",
  "description": "React Renderer package for EasyEditor.",
  "type": "module",
  "main": "src/index.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "typings": "dist/index.d.ts",
    "module": "dist/index.js",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/cjs/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "homepage": "https://github.com/Easy-Editor/EasyEditor",
  "license": "MIT",
  "author": "JinSo <kimjinso@qq.com>",
  "contributors": [
    {
      "name": "NoahCodeGG",
      "email": "noahcodegg@gmail.com",
      "url": "https://github.com/NoahCodeGG"
    }
  ],
  "keywords": [
    "@easy-editor",
    "easyeditor",
    "low-code",
    "editor",
    "engine",
    "dashboard",
    "react",
    "renderer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Easy-Editor/EasyEditor"
  },
  "bugs": {
    "url": "https://github.com/Easy-Editor/EasyEditor/issues"
  },
  "scripts": {
    "dev": "deno run --watch ./src/index.ts",
    "format": "biome format --write .",
    "lint": "biome check .",
    "build": "npm-run-all -nl build:*",
    "build:clean": "rimraf dist/",
    "build:js": "rollup -c",
    "types": "npm-run-all -nl types:*",
    "types:src": "tsc --project tsconfig.build.json",
    "test-types": "tsc --project tsconfig.test.json"
  },
  "peerDependencies": {
    "@easy-editor/core": "^0.0.6",
    "mobx-react": "^9.2.0",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "@types/react": "^18 || ^19",
    "@types/react-dom": "^18 || ^19"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "prop-types": "^15.8.1",
    "react-is": "^18.3.1"
  },
  "devDependencies": {
    "@easy-editor/core": "workspace:*",
    "@types/lodash-es": "^4.17.12",
    "@types/prop-types": "^15.7.14",
    "@types/react-is": "^18.3.0"
  }
}
