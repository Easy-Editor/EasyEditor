{
  "name": "@easy-editor/plugin-materials-dashboard",
  "version": "0.0.0",
  "description": "Materials dashboard plugin for EasyEditor.",
  "type": "module",
  "main": "src/index.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "typings": "dist/index.d.ts",
    "module": "dist/index.js",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/cjs/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "homepage": "https://github.com/Easy-Editor/EasyEditor",
  "license": "MIT",
  "author": "JinSo <kimjinso@qq.com>",
  "keywords": [
    "@easy-editor",
    "easyeditor",
    "low-code",
    "editor",
    "engine",
    "plugin",
    "dashboard"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Easy-Editor/EasyEditor"
  },
  "bugs": {
    "url": "https://github.com/Easy-Editor/EasyEditor/issues"
  },
  "scripts": {
    "dev": "deno run --watch ./src/index.ts",
    "format": "biome format --write .",
    "lint": "biome check .",
    "build": "npm-run-all -nl build:*",
    "build:clean": "rimraf dist/",
    "build:js": "rollup -c",
    "types": "npm-run-all -nl types:*",
    "types:src": "tsc --project tsconfig.build.json",
    "test-types": "tsc --project tsconfig.test.json"
  },
  "peerDependencies": {
    "@easy-editor/core": "workspace:*",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.475.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.2",
    "tailwindcss": "3"
  }
}
